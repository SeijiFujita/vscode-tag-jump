{
    "name": "vscode-tag-jump",
    "displayName": "Tag-Jump",
    "description": "Add Tag-Jump function(Open a file with tag information /file.txt:10/) to vscode.",
    "version": "0.0.1",
    "publisher": "sfujita",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/tagjump-logo300x.png",
    "galleryBanner": {
        "color": "#eeeeee",
        "theme": "light"
    },
    "license": "MIT",
    "activationEvents": [
        "onCommand:extension.tagJump"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.tagJump",
            "title": "Tag-Jump: Open a file with tag information"
        }],
       "configuration": {
            "properties": {
                "tagjump.bingTransId": {
                    "default": "tagjump",
                    "description": "Your client id",
                    "type": "string"
                }
            },
            "title": "Tag-Jump Configuration",
            "type": "object"
        },
        "menus": {
            "editor/context": [
                {
                    "command": "extension.tagJump",
                    "when": "editorFocus"
                }
            ]
        },
        "keybindings": [
            {
                "command": "extension.tagJump",
                "key": "Alt+T"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}