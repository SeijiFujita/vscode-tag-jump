{
    "name": "vscode-tag-jump",
    "displayName": "Tag-Jump",
    "description": "Add Tag-Jump function(Open a file with tag information /file.txt:10/) to vscode.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "publisher": "SeijiFujita",
    "icon": "images/tagjump-logo300x.png",
    "galleryBanner": {
        "color": "#eeeeee",
        "theme": "light"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SeijiFujita/vscode-tagjump/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SeijiFujita/vscode-tagjump.git"
    },
    "homepage": "https://github.com/SeijiFujita/vscode-tagjump",
    "keywords": [
        "vscode-tagjump",
        "tagjump",
        "tag-jump",
        "extension"
    ],
    "activationEvents": [
        "onCommand:extension.tagJump"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.tagJump",
            "title": "Tag-Jump: Open a file with tag information"
        }],
       "configuration": {
            "title": "Tag-Jump Configuration",
            "type": "object",
            "properties": {
                "tagjump.quiet": {
                    "default": false,
                    "description": "do not display an error message",
                    "type": "boolean"
                }
            }
        },
        "menus": {
            "editor/context": [
                {
                    "command": "extension.tagJump",
                    "when": "editorFocus"
                }
            ]
        },
        "keybindings": [
            {
                "key": "Alt+f10",
                "command": "extension.tagJump",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}